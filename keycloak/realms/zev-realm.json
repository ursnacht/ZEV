{
    "realm": "zev",
    "enabled": true,
    "clients": [
        {
            "clientId": "zev-frontend",
            "enabled": true,
            "publicClient": true,
            "standardFlowEnabled": true,
            "implicitFlowEnabled": false,
            "directAccessGrantsEnabled": false,
            "serviceAccountsEnabled": false,
            "redirectUris": [
                "http://localhost:4200/*"
            ],
            "webOrigins": [
                "http://localhost:4200"
            ],
            "attributes": {
                "pkce.code.challenge.method": "S256"
            },
            "defaultClientScopes": [
                "openid",
                "profile",
                "email",
                "roles",
                "web-origins"
            ],
            "optionalClientScopes": [
                "organization"
            ]
        }
    ],
    "users": [
        {
            "username": "user",
            "enabled": true,
            "credentials": [
                {
                    "type": "password",
                    "value": "password"
                }
            ],
            "realmRoles": [
                "user",
                "zev"
            ]
        },
        {
            "username": "testuser",
            "enabled": true,
            "emailVerified": true,
            "credentials": [
                {
                    "type": "password",
                    "value": "testpassword"
                }
            ],
            "realmRoles": [
                "user",
                "zev",
                "zev_admin"
            ]
        }
    ],
    "roles": {
        "realm": [
            {
                "name": "user",
                "description": "User role"
            },
            {
                "name": "zev",
                "description": "ZEV Member role"
            },
            {
                "name": "zev_admin",
                "description": "ZEV Admin role"
            }
        ]
    }
}